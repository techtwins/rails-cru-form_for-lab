<h1>Edit Song</h1>
<%= form_for(@song) do |f| %>
    <div>
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </div>
    <div>
        <%= f.label :artist_id %><br>
        <%= f.text_field :artist_id %>
    </div>
    <div>
        <%= f.label :genre_id %><br>
        <%= f.text_field :genre_id %>
    </div>
    <br>
        <%= f.submit %>
<% end %>
